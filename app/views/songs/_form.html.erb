<h1><%= @song.id ? 'Edit Song' : 'New Song' %></h1>


<%= form_for @song do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title %><br>

  <%= f.label :artist %>
  <%= f.text_field :artist %><br>

  <%= f.label :genre %>
  <%= f.text_field :genre %><br>

  <%= f.label :rank %>
  <%= f.text_field :rank %><br>

  <%= f.submit %>
<% end %>
